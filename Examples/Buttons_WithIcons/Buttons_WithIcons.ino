void setup() {

  Serial.begin(57600);

  // fire up the display
  Display.begin();
  Display.setRotation(1);
  Display.fillScreen(C_BLACK);
  analogWrite(A9, 255);
  // fire up the touch display

  // you may need this depending on your display
  Touch.begin();

  Touch.setRotation(3);
  ButtonIcon.init(180, 140, 56, 56, C_BLACK, C_BLACK, C_BLACK, C_BLACK, Settings, Settings_Pressed, 50, 50, 3, 3);
  ButtonIcon.setCornerRadius(2);

  ButtonIcon.draw();



}


void loop() {

delay(10);
